{
    "name": "foostart/package-acl",
    "description": "Laravel authentication and ACL package with a cool admin panel. Laravel 7x",
    "keywords": [
        "acl",
        "authentication",
        "signup",
        "admin panel",
        "profile",
        "laravel"
    ],
    "authors": [
        {
            "name": "foostart",
            "email": "foostart.com@gmail.com",
            "homepage": "http://www.foostart.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravelcollective/html": "6.2.1",
        "intervention/image": "2.*",
        "foostart/package-category": "*",
        "foostart/package-sentry": "*",
        "gregwar/captcha": "1.1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "~1.4",
        "sebastian/exporter": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Foostart\\Acl\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/unit"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
